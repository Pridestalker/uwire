{
  "require": {
    {{#requiredPackages}}
      {{#.}}
        "{{ package }}": "{{ version }}"{{#hasComma}},{{/hasComma}}
      {{/.}}
    {{/requiredPackages}}
  },
  "require-dev": {
    {{#devPackages}}
      {{#.}}
        "{{ package }}": "{{ version }}"{{#hasComma}},{{/hasComma}}
      {{/.}}
    {{/devPackages}}
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "vendor/aristath/kirki/kirki.php",
      "src/app.php"
    ]
  }
}
