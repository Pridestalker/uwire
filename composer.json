{
    "name": "elderbraum/uwire",
    "description": "The uWire Theme Framework.",
    "keywords": [],
    "homepage": "https://uwire.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Pridestalker/uwire/issues",
        "source": "https://github.com/Pridestalker/uwire"
    },
    "authors": [
        {
            "name": "Mitch Hijlkema",
            "email": "mitchhijlkema@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "laravel-zero/framework": "5.8.*",
        "mustache/mustache": "^2.12",
        "nunomaduro/laravel-console-menu": "^2.1",
        "padraic/phar-updater": "^1.0.6",
        "zendframework/zend-text": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/uwire"]
}
